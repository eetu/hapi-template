{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [true, 200],
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "interface-name": [true, "never-prefix"]
  },
  "rulesDirectory": []
}
